{
    "name": "elementworks/send-password-reset",
    "description": "Add send password reset email action to users element index page action menu",
    "type": "craft-plugin",
    "version": "1.0.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "send password reset"
    ],
    "support": {
        "docs": "https://github.com/elementworks/send-password-reset/blob/main/README.md",
        "issues": "https://github.com/elementworks/send-password-reset/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Steve Rowling",
            "homepage": "https://springworks.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "elementworks\\sendpasswordreset\\": "src/"
        }
    },
    "extra": {
        "name": "Send Password Reset",
        "handle": "send-password-reset",
        "hasCpSettings": false,
        "hasCpSection": false,
        "developer": "Element Works Ltd",
        "developerUrl": "https://elementworks.co.uk",
        "documentationUrl": "https://github.com/elementworks/send-password-reset/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/elementworks/send-password-reset/master/CHANGELOG.md",
        "class": "elementworks\\sendpasswordreset\\SendPasswordReset"
    }
}
